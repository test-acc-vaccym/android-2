<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/WhiteSmoke"
    android:layout_marginBottom="@dimen/app_bar_height">
    <com.edroplet.qxx.saneteltabactivity.view.custom.CustomTextView
        android:id="@+id/welcome_fix_device_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/app_bar_height"
        android:gravity="center"
        android:text="@string/welcome_fix_device"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"/>
    <com.edroplet.qxx.saneteltabactivity.view.custom.CustomTextView
        android:padding="@dimen/layout_padding_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/welcome_fix_device_detail"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"/>
</LinearLayout>